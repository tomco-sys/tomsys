companies controller pre_edit view

<%= link_to "ホームへ", root_path, class: 'get' %>

<!-- Editable table -->
<div class="card">
  <h3 class="card-header text-center font-weight-bold text-uppercase py-4">登録済得意先リスト</h3>
  <div class="card-body">
    <div id="table" class="table-editable">
      <span class="table-add float-right mb-3 mr-2"><a href="#!" class="text-success"><i
            class="fas fa-plus fa-2x" aria-hidden="true"></i></a></span>
      <table class="table table-bordered table-responsive-md table-striped text-center">
        <thead>
          <tr>
            <th class="text-center">id</th>
            <th class="text-center">会社名</th>
            <th class="text-center">会社名(ｶﾅ)</th>
            <th class="text-center">照会</th>
            <th class="text-center">削除</th>
          </tr>
        </thead>

        <% @companys.each do |comp| %>
        <tbody>
          <tr>
            <td class="pt-3-half" contenteditable="true"><%= comp.id %></td>
            <td class="pt-3-half" contenteditable="true"><%= comp.company_name %></td>
            <td class="pt-3-half" contenteditable="true"><%= comp.company_kana %></td>
            <td>
              <span class="table-remove"><a href="/companies/<%= comp.id %>/" type="button"
                class="btn btn-info btn-rounded  btn0sm my-0">照会</a></span>
            </td>
            <td>
              <span class="table-remove"><%= link_to "削除", "/companies/#{comp.id}",class: 'btn btn-danger btn-rounded btn-sm my-0', method: :delete %></span>

            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%= paginate @companys %>
